{
  "dataset_path": "resources/tests/database/test.csv",
  "frequency": "1H",
  "forecast_window": 7,
  
  "preprocess": {
    "datetime": [
      "Date",
      "Time"
    ],
    "datetime_format": "%d/%m/%Y %I:%M %p",
    "null_list": [
      "---"
    ],
    "encode_time": {
      "day": 86400,
      "year": 31556952
    },
    "special_format": {
      "Leaf Wet 1": "int"
    },
    "special_functions":{
      "Leaf Wet 1": "max"
    }
  },

  "neural_network": {
    "model_name": "tcn_model",
    "nn_path": "resources/tests/checkpoints",
    "submodel": "tcn"
  }
}