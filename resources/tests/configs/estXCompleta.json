{
  "dataset_path": "resources/tests/database/estXCompleta.csv",
  "frequency": "1H",
  "forecast_window": 7,
  "output_path":"resources/tests/results",

  "preprocess": {
    "datetime": [
      "Date",
      "Time"
    ],
    "datetime_format": "%d/%m/%Y %I:%M %p",
    "null_list": [
      "---",
      "------"
    ],
    "delete_columns": [
      "Dew Pt.",
      "Wind Dir",
      "Wind Run",
      "Hi Dir",
      "Wind Chill",
      "Heat Index",
      "THW Index",
      "THSW Index",
      "Rain Rate",
      "Solar Energy",
      "UV Index",
      "UV Dose",
      "Hi UV ",
      "Heat D-D ",
      "Cool D-D ",
      "In Dew",
      "In Heat",
      "In EMC",
      "In Air Density",
      "Temp 2nd",
      "Temp 3rd",
      "Temp 4th",
      "Hum 2nd",
      "Hum 3rd",
      "ET ",
      "Soils 2 Moist.",
      "Soils 3 Moist.",
      "Soils 4 Moist.",
      "Soil Temp 1",
      "Soil Temp 2",
      "Soil Temp 3",
      "Soil Temp 4",
      "Leaf Wet 2",
      "Leaf Temp 1",
      "Leaf Temp 2",
      "Wind Samp",
      "Wind Tx ",
      "ISS Recept",
      "Arc Int."
    ],
    "encode_time": {
      "day": 86400,
      "year": 31556952
    },
    "special_functions": {
      "Hi Temp": "max",
      "Low Temp": "min",
      "Hi Speed": "max",
      "Rain": "sum",
      "Hi Solar Rad. ": "max"
    },
    "special_format": {
      "Out Hum": "int",
      "Solar Rad.": "int",
      "Hi Solar Rad. ": "int",
      "In Hum": "int",
      "Soils 1 Moist.": "int",
      "Leaf Wet 1": "int"
    }
  },

    "neural_network": {
      "model_name": "Station_7",
      "nn_path": "resources/tests/checkpoints",
      "submodel": "tcn",

      "max_epochs": 100,
      "learning_rate": 0.0001,
      "patience": 4,
      "batch_size": 128
    }
}